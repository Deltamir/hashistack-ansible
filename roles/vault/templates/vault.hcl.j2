listener "tcp" {
  address         = "{{ inventory_hostname }}:8200"
  cluster_address = "{{ inventory_hostname }}:8201"
  tls_disable     = true
}

storage "consul" {
  address = "{{ inventory_hostname }}:8500"
  path    = "vault/"
}

ui            = true
disable_mlock = true
api_addr      = "http://{{ inventory_hostname }}:8200"
cluster_addr  = "http://{{ inventory_hostname }}:8201"
